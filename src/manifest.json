{
  "manifest_version": 2,
  "name": "Overleaf Commander",
  "description": "Shortcuts and commands for overleaf.com",
  "version": "0.2.2",
  "incognito": "split",
  "content_scripts": [
    {
      "matches": [
        "https://www.overleaf.com/project/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "app.js"
  ]
}
